	<queryString language="SQL">
		<![CDATA[SELECT
     penjualan.`id_penjualan` AS penjualan_id_penjualan,
     penjualan.`id_pelanggan` AS penjualan_id_pelanggan,
     penjualan.`tanggal` AS penjualan_tanggal,
     penjualan.`total_harga` AS penjualan_total_harga,
     penjualan.`id_karyawan` AS penjualan_id_karyawan,
     penjualan.`bayar` AS penjualan_bayar,
     penjualan.`kembali` AS penjualan_kembali,
     penjualan.`diskon` AS penjualan_diskon,
     penjualan.`create_at` AS penjualan_create_at,
     penjualan.`update_at` AS penjualan_update_at,
     penjualan_smt.`id_penjualan` AS penjualan_smt_id_penjualan,
     penjualan_smt.`id_produk` AS penjualan_smt_id_produk,
     penjualan_smt.`barcode` AS penjualan_smt_barcode,
     penjualan_smt.`nama_produk` AS penjualan_smt_nama_produk,
     penjualan_smt.`harga` AS penjualan_smt_harga,
     penjualan_smt.`stok` AS penjualan_smt_stok,
     penjualan_smt.`jumlah` AS penjualan_smt_jumlah,
     penjualan_smt.`subtotal` AS penjualan_smt_subtotal
FROM
     `penjualan` penjualan INNER JOIN `penjualan_smt` penjualan_smt ON penjualan.`id_penjualan` = penjualan_smt.`id_penjualan`]]>
